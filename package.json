{
  "name": "doc-web-client",
  "version": "0.0.1",
  "description": "Angular-based frontend to handle user interactions with the backend services, document management, ingestion management, and RAG-based Q&A interface",
  "author": "Ravi Raj",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "jest-preset-angular": "^15.0.0",
    "typescript": "~5.8.2"
  }
}
